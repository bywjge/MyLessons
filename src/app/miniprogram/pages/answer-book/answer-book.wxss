page {
  overflow: hidden;

  --border-radius: 28rpx;
  --button-icon-size: 70rpx;

  --icon-choose: url('data:image/svg+xml;base64,');
  --icon-dice: url('data:image/svg+xml;base64,');

  --icon-dice-1: url('data:image/svg+xml;base64,');
  --icon-dice-2: url('data:image/svg+xml;base64,');
  --icon-dice-3: url('data:image/svg+xml;base64,');
  --icon-dice-4: url('data:image/svg+xml;base64,');
  --icon-dice-5: url('data:image/svg+xml;base64,');
  --icon-dice-6: url('data:image/svg+xml;base64,');
  --dice-rotate-time: 4s;
}

::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
  scroll-behavior: smooth;
}

.container > .content {
  /* width: 90%; */
  /* 不然box-shadow会被截断 */
  width: 100%;
  box-sizing: border-box;
  padding: 0 9%;
  padding-top: 60rpx;
  padding-bottom: 50rpx;
  flex: 1;
  overflow: scroll;
}
.results {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  /* -webkit-filter: drop-shadow(0 0 6rpx var(--color-pink-2)); */
}
.results > .item, .battle-progress .item {
  --size: 50rpx;
  width: var(--size);
  height: var(--size);
  mask-size: var(--size) var(--size);
  -webkit-mask-size: var(--size) var(--size);
  background-color: var(--color-pink-1);
  margin-right: 10rpx;
  margin-bottom: 10rpx;
}

.square-button.wrong .inner {
  -webkit-filter: drop-shadow(0 0 6rpx var(--color-blue-2));
}
.results > .item.wrong, .square-button.wrong .inner:before {
  background-color: var(--color-blue-1);
}


.item.right {
  -webkit-mask-image: var(--icon-right);
}

.item.wrong {
  -webkit-mask-image: var(--icon-wrong);
}
.results > .item.dice-1, .cube > view.dice-1 .inner:before {
  -webkit-mask-image: var(--icon-dice-1);
}
.results > .item.dice-2, .cube > view.dice-2 .inner:before {
  -webkit-mask-image: var(--icon-dice-2);
}
.results > .item.dice-3, .cube > view.dice-3 .inner:before {
  -webkit-mask-image: var(--icon-dice-3);
}
.results > .item.dice-4, .cube > view.dice-4 .inner:before {
  -webkit-mask-image: var(--icon-dice-4);
}
.results > .item.dice-5, .cube > view.dice-5 .inner:before {
  -webkit-mask-image: var(--icon-dice-5);
}
.results > .item.dice-6, .cube > view.dice-6 .inner:before {
  -webkit-mask-image: var(--icon-dice-6);
}

.stage {
  transition: all .8s;
  /* animation: zoomIn ease 1s forwards; */
  opacity: 0;
  transform: translate(-10px, -10px) scale(.8);
}

.stage.choose {
  display: flex;
  align-items: center;
  justify-content: space-around;
  width: 80%;
  height: 300rpx;
  /* --delay: .3s; */
}

.stage.choose .square-button {
  transform: scale(1);
  /* transition: transform .3s ease; */
}

.stage.choose .square-button.result {
  opacity: 0;
  position: absolute;
  transition: transform .3s;
}
.stage.choose .square-button.result.show {
  opacity: 1;
  transform: translateY(-200%) scale(1.5);
}

.stage.choose .square-button.shake {
  animation: shekeAnimation .8s infinite linear;
  transform-origin: 50% 50%;
}

.stage.choose .square-button.bigger {
  transform: scale(1.4);
}

.container > .content .visible {
  animation: zoomIn ease .8s forwards !important;
}

.container > .content .invisible {
  animation: zoomOut ease .8s forwards;
}

.container > .content > .front {
  position: absolute;
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 100;
}

.container > .content .front .title {
  color: var(--text-black-2);
  font-size: 50rpx;
  font-weight: 500;
}

.container > .content .front .subtitle {
  text-align: center;
  color: var(--text-black-3);
  font-size: 24rpx;
  margin-top: 10rpx;
}

.container > .content .front .inner-wrapper {
  display: flex; 
  flex-direction: column; 
  align-items: center;
}

.container > .content .front .inner-wrapper .backgrounded-item {
  background: var(--item-background);
  padding: 10rpx 20rpx;
  border-radius: 10rpx;
}

.button {
  /* margin-top: 110rpx; */
  /* width: 100%; */
  width: 400rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  color: #FFFFFF;
  text-align: center;
  user-select: none;
  transition: all 0.3s;
  font-weight: 500;
  background: var(--button-background);
  border-radius: 11px;
}

.button-tap {
  background-color: var(--button-tap-background);
}

.front .button-wrapper {
  margin-top: 50rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 300rpx;
}

.square-button {
  position: relative;
  --size: 130rpx;
  width: var(--size);
  height: var(--size);
  background: var(--item-background);
  border-radius: var(--border-radius);
  box-shadow: 0 0 20rpx 0 var(--shadow-10);
  transition: all .3s;
}

.square-button-tap {
  box-shadow: 0 0 26rpx 0 var(--shadow-20);
}

.inner {
  width: 100%;
  height: 100%;
  filter: drop-shadow(0 0 6rpx var(--color-pink-2));
}

.inner:before {
  top: 50%;
  left: 50%;
  transform: translate3d(-50%, -50%, 0);
  content: '';
  position: absolute;
  width: var(--button-icon-size);
  height: var(--button-icon-size);
  mask-size: var(--button-icon-size) var(--button-icon-size);
  -webkit-mask-size: var(--button-icon-size) var(--button-icon-size);
  background: var(--color-pink-1);
}

.square-button.choose .inner:before {
  mask-image: var(--icon-choose);
  -webkit-mask-image: var(--icon-choose);
}

.square-button.dice .inner:before {
  mask-image: var(--icon-dice);
  -webkit-mask-image: var(--icon-dice);
}

.square-button.right .inner:before {
  mask-image: var(--icon-right);
  -webkit-mask-image: var(--icon-right);
}

.square-button.wrong .inner:before {
  mask-image: var(--icon-wrong);
  -webkit-mask-image: var(--icon-wrong);
}

/** 骰子 */
.cube-container {
  --cube-size: 200px;
  width: var(--cube-size);
  height: var(--cube-size);
  position: relative;
  /* border: 1px solid #FFF; */
  perspective: 1000px;
  -webkit-perspective: 1000px;
  perspective-origin: 50% 100%;
}

.cube-container > .cube {
  width: 100%;
  height: 100%;
  /* top: 100px; */
  position: absolute;
  transform-style: preserve-3d;
  transition: transform var(--dice-rotate-time);
}

.cube .front {
  transform: 
    translateZ(calc(var(--cube-size) / 2));
}

.cube .back {
  transform: 
    rotateX(-180deg)
    translateZ(calc(var(--cube-size) / 2));
}

.cube .right {
  transform:
    rotateY(90deg)
    translateZ(calc(var(--cube-size) / 2));
}

.cube .left {
  transform:
    rotateY(-90deg)
    translateZ(calc(var(--cube-size) / 2));
}

.cube .top {
  transform:
    rotateX(90deg)
    translateZ(calc(var(--cube-size) / 2));
}

.cube .bottom {
  transform:
    rotateX(-90deg)
    translateZ(calc(var(--cube-size) / 2));
}

.cube > view {
  background: var(--item-background);
  display: block;
  position: absolute;
  width: 200px;
  height: 200px;
  border: 3px solid var(--color-black-10);
  margin: 0 auto;  
  /* 解决层叠覆盖问题 */
  backface-visibility: hidden;
}
.cube > view .inner {
  filter: drop-shadow(0 0 16rpx var(--color-pink-2));
}
.cube > view .inner:before {
  height: 100%;
  width: 100%;
  -webkit-mask-size: var(--cube-size) var(--cube-size);
}

.restart {
  position: absolute;
  bottom: 200rpx; 
  left: 50%;
  transform: translateX(-50%);
}

.battle-progress {
  position: absolute;
  bottom: 100rpx;
  width: calc(100% - 9% * 2);
  display: inline-flex;
  align-items: center;
  justify-content: center;
}
.battle-progress .circle {
  position: relative;
  border-radius: 50%;
}
.battle-progress .circle text {
  position: absolute;
  color: var(--text-black-2);
  font-weight: 450;
  font-size: 28rpx;
  left: 50%;
  transform: translateX(-50%);
  top: 110%;
}
.battle-progress .circle .item {
  margin: 0;
  transform: scale(.6);
  background: #FFFFFF;
}

@keyframes zoomIn {
  from {
    opacity: 0;
    transform: translate(10px, 10px) scale(.8);
  }

  to {
    opacity: 1;
    transform: translate(0, 0) scale(1);
  }
}

@keyframes zoomOut {
  from {
    /* opacity: 1; */
    transform: translate(0, 0) scale(1);
  }

  to {
    opacity: 0;
    transform: translate(10px, 10px) scale(.8);
  }
}
