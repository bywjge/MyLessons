/* 日间模式 */
@media (prefers-color-scheme: light) {
  page {
    --page-welcome-background: #FFC0CB;
    --page-background: #F7F7F7;
    background: var(--page-background);

    --border-optional: transparent;

    /* 文本高亮色 */
    --highlight-font-color: #ff617c;

    /* 周视图：当日课程的竖条高亮色 */
    --lesson-strip-active: #ff2c6133;

    /* 菜单等高亮的背景 */
    --highlight-background: rgba(255, 0, 71, 0.1);
    /* 高亮的颜色 */
    --highlight-color: #ffa7a7;

    --navigation-bar-text: white;
    --navigation-bar-background: #FFB8C5;

    --card-background: white;
    --item-background: white;

    --card-underway: #FF7D7D;
    --card-normal: #66BFFF;
    --card-coming: #FFAF66;

    --button-background: #FFA6B5;
    --button-tap-background: #f092a2;
    --button-text: white;

    --color-white: white;
    --color-blue-1: #1fa0ea;
    --color-blue-2: #5DADE2;
    --color-blue-3: #8CB5D0;
    --color-blue-4: #B1C7DC;
    --color-pink-1: #ff617c;
    --color-pink-2: #FF8B9C;
    --color-pink-3: #ef8b9a;
    --color-green-1: #57C35A;
    --color-purple-1: #A42FFF;
    --color-red-1: #FF3030;
    --color-orange-1: #ff8733;

    --color-warning: #FF7227;

    --color-black-05: rgba(0, 0, 0, .05);
    --color-black-07: rgba(0, 0, 0, .07);
    --color-black-10: rgba(0, 0, 0, .1);
    --color-black-20: rgba(0, 0, 0, .2);
    --color-black-30: rgba(0, 0, 0, .3);
    --color-black-40: rgba(0, 0, 0, .4);
    --color-white-30: rgba(255, 255, 255, 0.3);
    --color-white-40: rgba(255, 255, 255, 0.4);
    --color-white-50: rgba(255, 255, 255, 0.5);
    --color-white-60: rgba(255, 255, 255, 0.6);
    --color-white-70: rgba(255, 255, 255, 0.7);
    --color-white-80: rgba(255, 255, 255, 0.8);

    --shadow-05: rgba(0, 0, 0, .05);
    --shadow-07: rgba(0, 0, 0, .07);
    --shadow-10: rgba(0, 0, 0, .1);
    --shadow-20: rgba(0, 0, 0, .2);
    --shadow-30: rgba(0, 0, 0, .3);
    --shadow-40: rgba(0, 0, 0, .4);

    --text-black-1: #2F2F2F;
    --text-black-2: #424242;
    --text-black-3: #727272;
    --text-black-4: #B1B1B1;
  }
}

/* 深色模式 */
@media (prefers-color-scheme: dark) {
  page {
    --page-welcome-background: #121212;
    --page-background: #121212;
    background: var(--page-background);

    --border-optional: rgba(255, 255, 255, .1);

    /* 文本高亮色 */
    --highlight-font-color: #e28494;

    /* 周视图：当日课程的竖条高亮色 */
    --lesson-strip-active: #ff2c6133;

    /* 菜单等高亮的背景 */
    --highlight-background: #f7699121;

    /* 高亮的颜色 */
    --highlight-color: #f95873;

    --navigation-bar-text: #b5b5b5;
    --navigation-bar-background: #252526;

    --card-background: #353535;
    --item-background: #1e1e1e;
    /* --item-background: rgba(255, 255, 255, .05); */

    --card-underway: #ce7d89;
    --card-normal: #5e87a1;
    --card-coming: #d4a57b;

    --button-background: #d47383;
    --button-tap-background: #b35867;
    --button-text: white;

    --color-white: #121212;
    --color-blue-1: #1f8fcf;
    --color-blue-2: #4f8eb7;
    --color-blue-3: #8CB5D0;
    --color-blue-4: #B1C7DC;
    --color-pink-1: #ff617c;
    --color-pink-2: #cc6373;
    --color-pink-3: #ef8b9a;
    --color-green-1: #449c47;
    --color-purple-1: #994ad6;
    --color-red-1: #b11c1c;
    --color-orange-1: #e25236;

    --color-warning: #FF7227;

    --color-black-05: rgba(255, 255, 255, .05);
    --color-black-07: rgba(255, 255, 255, .07);
    --color-black-10: rgba(255, 255, 255, .1);
    --color-black-20: rgba(255, 255, 255, .2);
    --color-black-30: rgba(255, 255, 255, .3);
    --color-black-40: rgba(255, 255, 255, .4);

    --color-white-30: rgba(0, 0, 0, 0.3);
    --color-white-40: rgba(0, 0, 0, 0.4);
    --color-white-50: rgba(0, 0, 0, 0.5);
    --color-white-60: rgba(0, 0, 0, 0.6);
    --color-white-70: rgba(0, 0, 0, 0.7);
    --color-white-80: rgba(0, 0, 0, 0.8);

    /* 深色模式下不应该出现阴影 */
    --shadow-05: transparent;
    --shadow-07: transparent;
    --shadow-10: transparent;
    --shadow-20: transparent;
    --shadow-30: transparent;
    --shadow-40: transparent;

    --text-black-1: #dbdbdb;
    --text-black-2: #c5c5c5;
    --text-black-3: #888888;
    --text-black-4: #666666;
  }
}
page {
  /* 防止影响到topbar
  overflow: hidden; */
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  height: 100%;
  width: 100%;
}

.container {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
}

button {
  background: initial;
}

input {
  border: none;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}

text[class="bold"]{
  font-weight: 700;
}

.disable {
  color: #888;
}

.dot-12px{
  width: 12px;
  height: 12px;
  border-radius: 6px;
  background-color: black;
}

@keyframes fadeIn{
  0%{
    opacity: 0;
  }
  100%{
    opacity: 1;
  }
}

@keyframes shekeAnimation {
  0% { transform: translate3d(2px, 1px, 0) rotate(0deg); }
  10% { transform: translate3d(-1px, -2px, 0) rotate(-1deg); }
  20% { transform: translate3d(-3px, 0px, 0) rotate(1deg); }
  30% { transform: translate3d(0px, 2px, 0) rotate(0deg); }
  40% { transform: translate3d(1px, -1px, 0) rotate(1deg); }
  50% { transform: translate3d(-1px, 2px, 0) rotate(-1deg); }
  60% { transform: translate3d(-3px, 1px, 0) rotate(0deg); }
  70% { transform: translate3d(2px, 1px, 0) rotate(-1deg); }
  80% { transform: translate3d(-1px, -1px, 0) rotate(1deg); }
  90% { transform: translate3d(2px, 2px, 0) rotate(0deg); }
  100% { transform: translate3d(1px, -2px, 0) rotate(-1deg); }
}