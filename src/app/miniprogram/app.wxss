/* global style */
@import './static/fonts/index.wxss';
/* ------------ */
@media (prefers-color-scheme: dark) {
  page {
    background: #181818;
  }
}
@media (prefers-color-scheme: light) {
  page {
    background: white;
  }
}
page {
  --icon-calendar: url('data:image/svg+xml;base64,');
  --icon-edit: url('data:image/svg+xml;base64,');
  --icon-reload: url('data:image/svg+xml;base64,');
  --icon-debug: url('data:image/svg+xml;base64,');
  --icon-code: url('data:image/svg+xml;base64,');

  --icon-list: url('data:image/svg+xml;base64,');
  --icon-target: url('data:image/svg+xml;base64,');
  --icon-rank: url('data:image/svg+xml;base64,');
  --icon-exam: url('data:image/svg+xml;base64,');
  --icon-gender-female: url('data:image/svg+xml;base64,');
  --icon-gender-male: url('data:image/svg+xml;base64,');
}

/* 日间模式 */
@media (prefers-color-scheme: light) {
  page {
    --page-welcome-background: #FFC0CB;
    --page-background: #F7F7F7;
    background: var(--page-background);

    /* 文本高亮色 */
    --highlight-font-color: #ff617c;

    /* 周视图：当日课程的竖条高亮色 */
    --lesson-strip-active: #ff2c6133;

    /* 菜单等高亮的背景 */
    --highlight-background: rgba(255, 0, 71, 0.1);
    /* 高亮的颜色 */
    --highlight-color: #FF7D7D;

    --navigation-bar-text: white;
    --navigation-bar-background: #FFB8C5;

    --card-background: white;

    --card-underway: #FF7D7D;
    --card-normal: #66BFFF;
    --card-coming: #FFAF66;

    --color-white: white;
    --color-blue-1: #1fa0ea;
    --color-blue-2: #5DADE2;
    --color-blue-3: #8CB5D0;
    --color-blue-4: #B1C7DC;
    --color-pink-1: #ff617c;
    --color-pink-2: #FF8B9C;
    --color-pink-3: #ef8b9a;
    --color-red-1: #FF3030;
    --color-green-1: #57C35A;
    --color-purple-1: #A42FFF;
    --color-red-1: #FF3030;
    --color-orange-1: #FF694C;

    --color-warning: #FF7227;

    --color-black-05: rgba(0, 0, 0, .05);
    --color-black-07: rgba(0, 0, 0, .07);
    --color-black-10: rgba(0, 0, 0, .1);
    --color-black-20: rgba(0, 0, 0, .2);
    --color-black-30: rgba(0, 0, 0, .3);
    --color-black-40: rgba(0, 0, 0, .4);
    --color-white-30: rgba(255, 255, 255, 0.3);
    --color-white-40: rgba(255, 255, 255, 0.4);
    --color-white-50: rgba(255, 255, 255, 0.5);
    --color-white-60: rgba(255, 255, 255, 0.6);
    --color-white-70: rgba(255, 255, 255, 0.7);
    --color-white-80: rgba(255, 255, 255, 0.8);

    --shadow-05: rgba(0, 0, 0, .05);
    --shadow-07: rgba(0, 0, 0, .07);
    --shadow-10: rgba(0, 0, 0, .1);
    --shadow-20: rgba(0, 0, 0, .2);
    --shadow-30: rgba(0, 0, 0, .3);
    --shadow-40: rgba(0, 0, 0, .4);
    
    --text-black-1: #2F2F2F;
    --text-black-2: #424242;
    --text-black-3: #727272;
    --text-black-4: #B1B1B1;
  }
}

/* 深色模式 */
@media (prefers-color-scheme: dark) {
  page {
    --page-welcome-background: #1d1d1d;
    --page-background: #1d1d1d;
    background: var(--page-background);

    /* 文本高亮色 */
    --highlight-font-color: #e28494;

    /* 周视图：当日课程的竖条高亮色 */
    --lesson-strip-active: #ff2c6133;

    /* 菜单等高亮的背景 */
    --highlight-background: #f7699121;
    /* 高亮的颜色 */
    --highlight-color: #f95873;

    --navigation-bar-text: #b5b5b5;
    --navigation-bar-background: #252526;

    --card-background: #353535;

    --card-underway: #ce7d89;
    --card-normal: #5e87a1;
    --card-coming: #d4a57b;

    --color-white: #121212;
    --color-blue-1: #1f8fcf;
    --color-blue-2: #4f8eb7;
    --color-blue-3: #8CB5D0;
    --color-blue-4: #B1C7DC;
    --color-pink-1: #ff617c;
    --color-pink-2: #cc6373;
    --color-pink-3: #ef8b9a;
    --color-red-1: #FF3030;
    --color-green-1: #449c47;
    --color-purple-1: #A42FFF;
    --color-red-1: #FF3030;
    --color-orange-1: #d4573e;

    --color-warning: #FF7227;

    --color-black-05: rgba(255, 255, 255, .05);
    --color-black-07: rgba(255, 255, 255, .07);
    --color-black-10: rgba(255, 255, 255, .1);
    --color-black-20: rgba(255, 255, 255, .2);
    --color-black-30: rgba(255, 255, 255, .3);
    --color-black-40: rgba(255, 255, 255, .4);

    --color-white-30: rgba(0, 0, 0, 0.3);
    --color-white-40: rgba(0, 0, 0, 0.4);
    --color-white-50: rgba(0, 0, 0, 0.5);
    --color-white-60: rgba(0, 0, 0, 0.6);
    --color-white-70: rgba(0, 0, 0, 0.7);
    --color-white-80: rgba(0, 0, 0, 0.8);

    /* 深色模式下不应该出现阴影 */
    --shadow-05: transparent
    --shadow-07: transparent;
    --shadow-10: transparent;
    --shadow-20: transparent;
    --shadow-30: transparent;
    --shadow-40: transparent;
    
    --text-black-1: #dbdbdb;
    --text-black-2: #c5c5c5;
    --text-black-3: #888888;
    --text-black-4: #666666;
  }
}
page {
  /* 防止影响到topbar
  overflow: hidden; */
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  height: 100%;
  width: 100%;
}

.container {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 

button {
  background: initial;
}

input {
  border: none;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}


text[class="bold"]{
  font-weight: 700;
}

.userinfo, .uploader, .tunnel {
  margin-top: 40rpx;
  height: 140rpx;
  width: 100%;
  background: #fff;
  border: 1px solid rgba(0, 0, 0, 0.1);
  border-left: none;
  border-right: none;
  display: flex;
  flex-direction: row;
  align-items: center;
  transition: all 300ms ease;
}

.userinfo-avatar {
  width: 100rpx;
  height: 100rpx;
  margin: 20rpx;
  border-radius: 50%;
  background-size: cover;
  background-color: white;
}

.userinfo-avatar:after {
  border: none;
}

.userinfo-nickname {
  font-size: 32rpx;
  color: #007aff;
  background-color: white;
  background-size: cover;
}

.userinfo-nickname::after {
  border: none;
}

.uploader, .tunnel {
  height: auto;
  padding: 0 0 0 40rpx;
  flex-direction: column;
  align-items: flex-start;
  box-sizing: border-box;
}

.uploader-text, .tunnel-text {
  width: 100%;
  line-height: 52px;
  font-size: 34rpx;
  color: #007aff;
}

.uploader-container {
  width: 100%;
  height: 400rpx;
  padding: 20rpx 20rpx 20rpx 0;
  display: flex;
  align-content: center;
  justify-content: center;
  box-sizing: border-box;
  border-top: 1px solid rgba(0, 0, 0, 0.1);
}

.uploader-image {
  width: 100%;
  height: 360rpx;
}

.tunnel {
  padding: 0 0 0 40rpx;
}

.tunnel-text {
  position: relative;
  color: #222;
  display: flex;
  flex-direction: row;
  align-content: center;
  justify-content: space-between;
  box-sizing: border-box;
  border-top: 1px solid rgba(0, 0, 0, 0.1);
}

.tunnel-text:first-child {
  border-top: none;
}

.tunnel-switch {
  position: absolute;
  right: 20rpx;
  top: -2rpx;
}

.disable {
  color: #888;
}

.service {
  position: fixed;
  right: 40rpx;
  bottom: 40rpx;
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  background: linear-gradient(#007aff, #0063ce);
  box-shadow: 0 5px 10px rgba(0, 0, 0, 0.3);
  display: flex;
  align-content: center;
  justify-content: center;
  transition: all 300ms ease;
}

.service-button {
  position: absolute;
  top: 40rpx;
}

.service:active {
  box-shadow: none;
}

.request-text {
  padding: 20rpx 0;
  font-size: 24rpx;
  line-height: 36rpx;
  word-break: break-all;
  margin-bottom: 82rpx;
}

.dot-12px{
  width: 12px;
  height: 12px;
  border-radius: 6px;
  background-color: black;
}

@keyframes fadeIn{
  0%{
    opacity: 0;
  }
  100%{
    opacity: 1;
  }
}

.hint{
  height: 100%;
  width: 100%;
}